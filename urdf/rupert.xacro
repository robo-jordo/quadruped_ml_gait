  </joint>

  <joint name="hip_j1" type="revolute">
    <parent link="block1"/>
    <child link="hip1"/>
    <origin xyz="${block_width/2} 0 0" rpy="0 0 -3.14"/>
    <axis xyz="0 0 1" />
    <limit effort="30" velocity="1.0" lower="-1.7" upper="1.7"/>
  </joint>

    <joint name="hip_j2" type="revolute">
