<?xml version="1.0"?>
<!-- Revolute-Revolute Manipulator -->
<robot name="rupert" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Constants for robot dimensions -->
  <xacro:property name="PI" value="3.1415926535897931"/>
  <xacro:property name="mass" value="1" /> <!-- arbitrary value for mass -->
  <xacro:property name="width" value="0.1" /> <!-- Square dimensions (widthxwidth) of beams -->
  <!--Body -->
  <xacro:property name="body_width" value="0.81" /> <!-- width of body -->
  <xacro:property name="body_length" value="1.13" /> <!-- length of body -->
  <xacro:property name="body_height" value="0.25" /> <!-- height of body -->

  <!--block -->
  <xacro:property name="block_width" value="0.22" /> <!-- width of body -->
  <xacro:property name="block_length" value="0.17" /> <!-- length of body -->
  <xacro:property name="block_height" value="0.25" /> <!-- height of body -->

  <!--Hip -->
  <xacro:property name="hip_width" value="0.4" /> <!-- width of body -->
  <xacro:property name="hip_length" value="0.4" /> <!-- length of body -->
  <xacro:property name="hip_height" value="0.4" /> <!-- height of body -->

  <!--leg -->
  <xacro:property name="leg_width" value="0.25" /> <!-- width of body -->
  <xacro:property name="leg_length" value="0.15" /> <!-- length of body -->
  <xacro:property name="leg_height" value="0.45" /> <!-- height of body -->

    <!--foot -->
  <xacro:property name="foot_width" value="0.01" /> <!-- width of body -->
  <xacro:property name="foot_length" value="0.01" /> <!-- length of body -->
  <xacro:property name="foot_height" value="0.75" /> <!-- height of body -->
  
  
  <xacro:property name="height1" value="0.0001" /> <!-- Link 1 -->
  <xacro:property name="height2" value="0.0005" /> <!-- Link 2 -->
  <xacro:property name="height3" value="0.0005" /> <!-- Link 3 -->
  <xacro:property name="camera_link" value="0.0005" /> <!-- Size of square 'camera' box -->
  <xacro:property name="axel_offset" value="0.5" /> <!-- Space btw top of beam and the each joint -->

  <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <xacro:include filename="$(find rupert_learns)/urdf/rupert.gazebo" />
  <!-- Import Rviz colors -->
  <xacro:include filename="$(find rupert_learns)/urdf/materials.xacro" />

  <!-- Used for fixing robot to Gazebo 'base_link' -->

<link name = "dummy"/>

<joint name="dummy_joint" type="fixed">
    <parent link="dummy"/>
    <child link="body"/>
</joint>

<link name="body">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
  <box size="${body_width} ${body_length} ${body_height}"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
  <box size="${body_width} ${body_length} ${body_height}"/>
      </geometry>
      <material name="black"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="${mass}"/>
      <inertia
    ixx="${mass / 12.0*(body_length*body_length + body_height*body_height)}" ixy="0.0" ixz="0.0"
    iyy="${mass / 12.0 * (body_height*body_height + body_width*body_width)}" iyz="0.0"
    izz="${mass / 12.0 * (body_width*body_width + body_length*body_length)}"/>
    </inertial>
  </link>

  <link name="block1">
