  <joint name="block_j1" type="fixed">
    <parent link="body"/>
    <child link="block1"/>
    <origin xyz="${body_width/2} ${body_length/2} 0" rpy="0 0 0.785398"/>
    <axis xyz="0 0 1" />
  </joint>

  <joint name="block_j2" type="fixed">
    <parent link="body"/>
    <child link="block2"/>
    <origin xyz="${body_width/2} ${-body_length/2} 0" rpy="0 0 -0.785398"/>
    <axis xyz="0 0 1" />
  </joint>

  <joint name="block_j3" type="fixed">
    <parent link="body"/>
    <child link="block3"/>
    <origin xyz="${-body_width/2} ${body_length/2} 0" rpy="0 0 -0.785398"/>
    <axis xyz="0 0 1" />
  </joint>

  <joint name="block_j4" type="fixed">
    <parent link="body"/>
    <child link="block4"/>
    <origin xyz="${-body_width/2} ${-body_length/2} 0" rpy="0 0 0.785398"/>
